{
  "name": "inito",
  "version": "2.2.2",
  "description": "Init React app with: webpack 5, wepback-dev-server, eslint, CSS Modules, proptypes, babel, react 18, react-router 6 with code splitting",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "node ./scripts/version.js",
    "build": "webpack --mode=production",
    "watch": "webpack --progress --watch",
    "start": "npm run build && webpack-dev-server --mode=development",
    "start:ui": "npx styleguidist server",
    "lint:js": "eslint src/ --cache",
    "prettier": "prettier --write 'src/**/*.js'",
    "git:remove": "rm -rf .git",
    "git:init": "git init",
    "generate:component": "node_modules/create-component-app/dist/index.js --template component --config ./config/templates/config.component.json",
    "generate:uicomponent": "node_modules/create-component-app/dist/index.js --template uicomponent --config ./config/templates/config.uicomponent.json",
    "generate:page": "node_modules/create-component-app/dist/index.js --template page --config ./config/templates/config.page.json"
  },
  "author": "Giovanni Bieller",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.3",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.30.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "i18next-hmr": "^1.7.7",
    "imagemin-webpack-plugin": "^2.4.2",
    "node-sass": "^7.0.0",
    "npm": "^6.14.17",
    "prettier": "^1.19.1",
    "react-dev-utils": "^12.0.1",
    "react-styleguidist": "^11.2.0",
    "readline": "^1.3.0",
    "style-loader": "^1.1.3",
    "webpack": "^5.73.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "create-component-app": "^1.7.0",
    "i18next": "^19.9.2",
    "i18next-xhr-backend": "^3.2.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.17.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0"
  },
  "cca": {
    "type": "component",
    "templatesDirPath": "./config/templates",
    "templateName": "component",
    "path": "./src/components"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giovannibieller/inito.git"
  }
}
